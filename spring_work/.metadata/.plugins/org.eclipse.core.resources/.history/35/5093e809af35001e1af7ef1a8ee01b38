package talgut;

public class bag {
	String name;
	int bags;
	
	
	
	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	public int getHanddles() {
		return bags;
	}



	public void setHanddles(int handdles) {
		this.handdles = handdles;
	}



	@Override
	public String toString() {
		
		return name+handdles+"ê°œ";
	}



	
	
}

package foodmenu;

public class Lunch {
	String lmain;
	int lmcal;
	String lside;
	int lscal;
	public Lunch(String lmain, int lmcal, String lside, int lscal) {
		super();
		this.lmain = lmain;
		this.lmcal = lmcal;
		this.lside = lside;
		this.lscal = lscal;
	}
	@Override
	public String toString() {
		return "Lunch [lmain=" + lmain + ", lmcal=" + lmcal + ", lside=" + lside + ", lscal=" + lscal + "]";
	}
	
}

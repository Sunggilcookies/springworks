package foodmenu;

public class dinner {
	String dmain;
	int dmcal;
	String dside;
	int dscal;
	public dinner(String dmain, String dside) {
		super();
		this.dmain = dmain;
		this.dside = dside;
	}
	@Override
	public String toString() {
		return "dinner [dmain=" + dmain + ", dside=" + dside + "]";
	}
	
}

package foodmenu;

public class Dinner {
	String dmain;
	int dmcal;
	String dside;
	int dscal;
	public Dinner(String dmain, int dmcal, String dside, int dscal) {
		super();
		this.dmain = dmain;
		this.dmcal = dmcal;
		this.dside = dside;
		this.dscal = dscal;
	}
	@Override
	public String toString() {
		return "Dinner [dmain=" + dmain + ", dmcal=" + dmcal + ", dside=" + dside + ", dscal=" + dscal + "]";
	}
	
	
}

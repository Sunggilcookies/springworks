package anno_p.copy;

import org.springframework.context.annotation.Configuration;
import org.springframework.stereotype.Component;

import anno_p.com.Bonche;
import anno_p.com.sub1.Mouse;
import anno_p.comabc.Monitor;

import org.springframework.context.annotation.Bean;


@Configuration
public class Copy {
	String coffe;
	Beean beean;
	Milk milk;
	Water water;
	int Price;
	
	public Copy() {
		// TODO Auto-generated constructor stub
	}

	public Copy(String coffe, Beean beean, Milk milk, Water water, int price) {
		super();
		this.coffe = coffe;
		this.beean = beean;
		this.milk = milk;
		this.water = water;
		Price = price;
	}
	public class Americano {
		String name;

		Bonche bon;
		
		Monitor m1, m2;
		
		Mouse mm;
		
		
		public Amercano(String name, Bonche bon, Monitor m1, Monitor m2, Mouse mm) {
			super();
			this.name = name;
			this.bon = bon;
			this.m1 = m1;
			this.m2 = m2;
			this.mm = mm;
		}

	}

	
	

	@Override
	public String toString() {
		return "Copy [coffe=" + coffe + ", beean=" + beean + ", milk=" + milk + ", water=" + water + ", Price=" + Price
				+ "]";
	}
	

	
}
@Component
class Caramel {
	String name="카라멜";
	String type="초코카라멜";
	
	public Caramel(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "Caramel [name=" + name + ", type=" + type + "]";
	}
}
@Component
class Milk {
	String name="우유", type="서울우유";

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Milk(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	@Override
	public String toString() {
		return "Milk [name=" + name + ", type=" + type + "]";
	}
	
}
@Component
class Water {
	String name="생수";

	public Water(String name) {
		super();
		this.name = name;
	}

	@Override
	public String toString() {
		return "Water [name=" + name + "]";
	}
	
}
@Component
class Beean {
	String name="원두";
	String type="아라비아";
	
	
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getType() {
		return type;
	}


	public void setType(String type) {
		this.type = type;
	}

	
	
	
	@Override
	public String toString() {
		return "Beean [name=" + name + ", type=" + type + "]";
	}

	
	
	
	
}

	


 




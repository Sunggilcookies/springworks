package anno_p.copy;

import org.springframework.context.annotation.Configuration;
import org.springframework.stereotype.Component;

import anno_p.copy.sub.Beean;
import anno_p.copy.sub.Milk;
import anno_p.copy.sub.Water;

import org.springframework.context.annotation.Bean;


@Configuration
public class Copy {
	@Bean
	String coffe;
	Beean beean;
	Milk milk;
	Water water;
	int Price;
	
	public Copy() {
		// TODO Auto-generated constructor stub
	}
	public Copy(String coffe, Beean beean, Milk milk, Water water) {
		super();
		this.coffe = coffe;
		this.beean = beean;
		this.milk = milk;
		this.water = water;
	}
	
	
}
@Component
class Caramel {
	String name="카라멜";
	String type="초코카라멜";
	
	public Caramel(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "Caramel [name=" + name + ", type=" + type + "]";
	}
}
@Component
class Milk {
	String name="우유", type="서울우유";

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Milk(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	@Override
	public String toString() {
		return "Milk [name=" + name + ", type=" + type + "]";
	}
	
}
@Component
class Water {
	String name="생수";

	public Water(String name) {
		super();
		this.name = name;
	}

	@Override
	public String toString() {
		return "Water [name=" + name + "]";
	}
	
}

	


 




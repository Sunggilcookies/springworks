package aop_p.grade;

import org.springframework.stereotype.Component;

@Component
public class Ban {
	
	

	public void 직장인반(int korean, int english) {
		System.out.println("직장인반 국어성적:"+korean+"영어성적:"+english);
	}
	
	public void 일반(int korean, int english, int math) {
		System.out.println("일반 국어성적:"+korean+"영어성적:"+english+"수학성적:"+math);
	}
	
	public String 예체능(int korean, int english, int math, int health) {
		System.out.println("예체능반 국어성적:"+korean+"영어성적:"+english+"건강점수:"+health);
		return "예체능반!";
	}

	
}

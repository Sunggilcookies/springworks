package talgut;

public class bag {
	String name;
	int bags;
	
	
	
	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	public int getBag() {
		return bags;
	}



	public void setBag(int bags) {
		this.bags = bags;
	}



	@Override
	public String toString() {
		
		return name+bags+"ê°œ";
	}



	
	
}

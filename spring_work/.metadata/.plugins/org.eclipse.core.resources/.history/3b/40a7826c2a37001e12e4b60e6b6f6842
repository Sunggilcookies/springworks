package foodmenu;

import foodmenu.*;


public class Menu {
	
	public Breakfast border_kr() {
		
		return new Breakfast("쌀죽",200,"생수",0);
	}
	public Lunch lorder_kr() {
		
		return new Lunch("제육볶음",1500,"과일주스",200);
	}
	public Dinner dorder_kr() {
		
		return new Dinner("백숙",1200,"육개장",2000);
	}
	
	
	
}

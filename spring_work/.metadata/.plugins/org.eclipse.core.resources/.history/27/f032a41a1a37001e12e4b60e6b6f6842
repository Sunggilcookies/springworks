package di_p.main;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import di_p.CoffeeShop;
import di_p.HambugerSet;


public class Lookup_main {

	public static void main(String[] args) {
		
		AbstractApplicationContext context = 
				new ClassPathXmlApplicationContext("di_xml/lookup.xml");

		
		CoffeeShop z = context.getBean("co",CoffeeShop.class);
		System.out.println("order_am:"+z.order_am());
		
	}

}

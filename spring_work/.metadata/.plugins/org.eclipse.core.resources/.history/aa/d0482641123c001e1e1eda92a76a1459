package aop_p.dohung;

import java.util.Arrays;

import org.aspectj.lang.ProceedingJoinPoint;
import org.springframework.stereotype.Component;

@Component
public class Circle {
	Object arTest(ProceedingJoinPoint joinPoint) {
			
	
	
	double 원넓이 =0;
	double 원둘레 =0;
	double PI= Math.PI;
	
	
	 Double 원넓이(double r) {
		원넓이 = r*r*PI;
		return 원넓이;
	}
	
	 Double 둘레의합계(double r, Double PI ) {
		원둘레 = r*2*PI;
		return 원둘레;
	
	
	
	}

	@Override
	public String toString() {
		return "Circle []";
	}	
}

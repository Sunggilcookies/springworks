package aop_p.hansick;

import org.aspectj.lang.JoinPoint;
import org.aspectj.lang.annotation.After;
import org.aspectj.lang.annotation.Around;
import org.aspectj.lang.annotation.Aspect;
import org.aspectj.lang.annotation.Pointcut;
import org.springframework.stereotype.Service;

@Service
@Aspect
public class HansickAdvice{
	@After("execution(* aop_p.hansik..* (..))")
	public void jj(JoinPoint joinpoint, String 찌개, int  가격) {
		System.out.println("hansick");
	}

	@Pointcut("execution(* aop_p.hansik..* (..))")
	public void pppiii() {}
	
	@After("pppiii()")
	public void dd(JoinPoint joinPoint ) {
		System.out.println("a");
	}
	


}
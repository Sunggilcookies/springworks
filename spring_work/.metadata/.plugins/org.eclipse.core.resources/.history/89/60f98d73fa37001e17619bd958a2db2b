package rby;

public class Red  {
	String name, score;
	int grade;
	public String getScore() {
		return score;
	}
	public void setScore(String score) {
		this.score = score;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		this.grade = grade;
	}
	@Override
	public String toString() {
		return "Red [name=" + name  + ", grade=" + grade + ", score=" + score+"]";
	}
	
}

class Blue{
	String name, score;
	int grade;
	public String getScore() {
		return score;
	}
	public void setScore(String score) {
		this.score = score;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		this.grade = grade;
	}
	@Override
	public String toString() {
		return "Blue [name=" + name  + ", grade=" + grade + ", score=" + score+"]";
	}
}

class Yellow{
	String name, score;
	public String getScore() {
		return score;
	}
	public void setScore(String score) {
		this.score = score;
	}
	int grade;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGrade() {
		return grade;
	}
	public void setGrade(int grade) {
		this.grade = grade;
	}
	@Override
	public String toString() {
		return "Yellow [name=" + name  + ", grade=" + grade + ", score=" + score+"]";
	}
	
}
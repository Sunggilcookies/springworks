package aop_p.grade;

import org.springframework.stereotype.Component;

@Component
public class Ban {
	
	

	public String 직장인반(int korean, int english) {
		System.out.println("직장인반 국어성적:"+korean+"영어성적"+english);
		return "직장인반!";
	}
	
	public String 일반(int korean, int english, int math) {
		System.out.println("직장인반 국어성적:"+korean+"영어성적"+english+"수학성적"+math);
		return "직장인반!";
	}
	
	public String 예체능(int korean, int english) {
		System.out.println("직장인반 국어성적:"+korean+"영어성적"+english);
		return "직장인반!";
	}

	
}

package talgut;

public class wheel {
	int i=0;

	public int getI() {
		return i;
	}

	public void setI(int i) {
		this.i = i;
	}

	@Override
	public String toString() {
		return "[바퀴의 개수는" + i + "개]";
	}
}

public class handdle {
	int i=0;

	public int getI() {
		return i;
	}

	public void setI(int i) {
		this.i = i;
	}

	@Override
	public String toString() {
		return "[핸들의 개수는" + i + "개]";
	}
}

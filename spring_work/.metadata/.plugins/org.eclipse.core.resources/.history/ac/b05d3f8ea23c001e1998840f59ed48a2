package aop_p.grade;

import org.springframework.stereotype.Component;

@Component
public class Ban {
	
	

	public String 직장인반(int people, int korean, int english) {
		System.out.println("직장인 반의 인원"+people+"평균 국어성적:"+korean+"평균 영어성적:"+english);
		return null;
		
	}
	
	public String 일반(int people, int korean, int english, int math) {
		System.out.println("일반 국어성적:"+korean+"영어성적:"+english+"수학성적:"+math);
		return "일반!";
	}
	
	public String 예체능(int people, int korean, int english, int math, int health) {
		System.out.println("예체능반 국어성적:"+korean+"영어성적:"+english+"건강점수:"+health);
		return "예체능반!";
	}

	
}

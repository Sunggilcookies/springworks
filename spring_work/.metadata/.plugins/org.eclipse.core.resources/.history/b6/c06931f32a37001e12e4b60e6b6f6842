package di_p.main;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;


import foodmenu.*;

public class Foodmenu_main {
	

		public static void main(String[] args) {
			
			AbstractApplicationContext context = 
					new ClassPathXmlApplicationContext("di_xml/foodmenu.xml");
		
		Menu kmenu = context.getBean("kmenu",Menu.class);
		System.out.println(kmenu.border_kr());
		System.out.println(kmenu.lorder_kr());
		System.out.println(kmenu.dorder_kr());
		Menu jmenu = context.getBean("jmenu",Menu.class);
		System.out.println(jmenu.border_kr());
		System.out.println(jmenu.lorder_kr());
		System.out.println(jmenu.dorder_kr());
			
		

	}

}

package rby;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import di_p.Coffee;
import di_p.CoffeeShop;

public class LifeCycle_main {

	public static void main(String[] args) {

		AbstractApplicationContext context = new ClassPathXmlApplicationContext("di_xml/rby.xml");
		System.out.println("=============main===============");
		System.out.println(context.getBean("r1"));
		System.out.println(context.getBean("r2"));
		System.out.println(context.getBean("b1"));
		System.out.println(context.getBean("b2"));
		System.out.println(context.getBean("y1"));
		System.out.println(context.getBean("y2"));
		System.out.println("================================");
		context.close();
		
	}

}

package talgut;

public class bag {
	String name;
	int bags;
	
	
	
	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = "가방";
	}



	public int getHanddles() {
		return bags;
	}



	public void setHanddles(int handdles) {
		this.bags = handdles;
	}



	@Override
	public String toString() {
		
		return name+bags+"개";
	}



	
	
}

package anno_p;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

public class Coffee {
	@Autowired(required = true)
	String name;
	@Autowired(required = true)
	int price;
	CoffeeBean cb;
	@Autowired
	@Qualifier("mmm2")
	Side sd;
}

class CoffeeBean{
	String name;
	String taste;
	
}
class Side{
	String name;
	int a;
}


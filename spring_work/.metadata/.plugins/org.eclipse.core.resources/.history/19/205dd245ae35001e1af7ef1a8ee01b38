package talgut;

public class item {

    public static class Wheel {
        int i = 0;

        public int getI() {
            return i;
        }

        public void setI(int i) {
            this.i = i;
        }

        @Override
        public String toString() {
            return "[바퀴의 개수는 " + i + "개]";
        }
    }

    public static class Handle {
        int i = 0;

        public int getI() {
            return i;
        }

        public void setI(int i) {
            this.i = i;
        }

        @Override
        public String toString() {
            return "[핸들의 개수는 " + i + "개]";
        }
    }

    public static class Bag {
        int i = 0;

        public int getI() {
            return i;
        }

        public void setI(int i) {
            this.i = i;
        }

        @Override
        public String toString() {
            return "[바구니의 개수는 " + i + "개]";
        }
    }
}

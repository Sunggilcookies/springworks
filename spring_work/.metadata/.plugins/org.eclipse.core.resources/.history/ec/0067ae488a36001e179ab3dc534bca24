public class Grade {
	Student student;
	int rank;
	
	public Grade(Student student, int rank) {
		super();
		this.student = student;
		this.rank = rank;
		rankCalc();
	}
	
	void rankCalc() { // 아무 배열을 집어넣었을때 배열인덱스만큼 반복
	      rank = 1;
	      for (Student i : exArr) { // exArr[you]이런 뜻임
	         if (student.arg < student.arg) {
	            rank++;
	         }
	      }
	   }

	@Override
	public String toString() {
		return "Grade [student=" + student + ", rank=" + rank + "]";
	}


}



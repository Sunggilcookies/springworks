package talgut;

public class wheel {
	String name;
	int wheels;
	
	
	
	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = "바퀴";
	}



	public int getWheels() {
		return wheels;
	}



	public void setWheels(int wheels) {
		this.wheels = wheels;
	}



	@Override
	public String toString() {
		
		return name+wheels+"개";
	}



	
	
}

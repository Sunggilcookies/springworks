package anno_p.copy;

import org.springframework.context.annotation.Configuration;
import org.springframework.stereotype.Component;

import anno_p.com.Bonche;
import anno_p.com.sub1.Mouse;
import anno_p.comabc.Monitor;

import org.springframework.context.annotation.Bean;


@Configuration
public class Copy {
	Americano americano() {
		return new Americano("아메리카노", "기본원두", null, "생수", 1500)
	}
	
	
	
		
		
		


	
}

class Americano {
	String name;

	Beean abeean;
	Milk amilk;
	Water awater;
	int Price;
	
	public Americano(String name, Beean abeean, Milk amilk, Water awater, int price) {
		super();
		this.name = name;
		this.abeean = abeean;
		this.amilk = amilk;
		this.awater = awater;
		Price = price;
	}

@Component
class Caramel {
	String name="카라멜";
	String type="초코카라멜";
	
	public Caramel(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "Caramel [name=" + name + ", type=" + type + "]";
	}
}
@Component
class Milk {
	String name="우유", type="서울우유";

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Milk(String name, String type) {
		super();
		this.name = name;
		this.type = type;
	}

	@Override
	public String toString() {
		return "Milk [name=" + name + ", type=" + type + "]";
	}
	
}
@Component
class Water {
	String name="생수";

	public Water(String name) {
		super();
		this.name = name;
	}

	@Override
	public String toString() {
		return "Water [name=" + name + "]";
	}
	
}
@Component
class Beean {
	String name="원두";
	String type="아라비아";
	
	
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getType() {
		return type;
	}


	public void setType(String type) {
		this.type = type;
	}

	
	
	
	@Override
	public String toString() {
		return "Beean [name=" + name + ", type=" + type + "]";
	}

	
	
	
	
}

	


 




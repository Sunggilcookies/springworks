package foodmenu;

public class Breakfast {
	String bmain;
	int bmcal;
	String bside;
	int bscal;
	public Breakfast(String bmain, int bmcal, String bside, int bscal) {
		super();
		this.bmain = bmain;
		this.bmcal = bmcal;
		this.bside = bside;
		this.bscal = bscal;
	}
	@Override
	public String toString() {
		return "breakfast [bmain=" + bmain + ", bmcal=" + bmcal + ", bside=" + bside + ", bscal=" + bscal + "]";
	}
	
}

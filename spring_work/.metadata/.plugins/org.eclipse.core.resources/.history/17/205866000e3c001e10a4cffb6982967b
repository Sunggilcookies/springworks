package aop_p.dohung;

public class Circle {
	double 넓이 =0;
	Double PI= Math.PI;
	public Double 넓이(double r, Double PI ) {
		넓이 = r*r*PI;
		return 넓이;
	}
	
	public Double 둘레의합계(double r, Double PI ) {
		넓이 = r*r*PI;
		return 넓이;
	}
	
	public int papa(int c) {
		System.out.println("조철왕:"+c);
		return 4000;
	}
}

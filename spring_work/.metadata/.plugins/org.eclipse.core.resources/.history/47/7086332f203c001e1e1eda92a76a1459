public class test1_main {
	public static void main(String[] args) {
		//40번째는 2*i=
		for(int i=0;i<=100;i+=2) {
			if(i*2==40) {
				System.out.println(i);
			}
			
		}
		int a=1;
		for(int j=1;j<=100;j+=2) {
			
			a++;
			if(j==67) {
				System.out.println(a);
			}
			
		}
		int[] five= new int [5];
		
		
	
}}
